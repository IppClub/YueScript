{
	"name": "e",
	"version": "0.26.0",
	"description": "A Moonscript dialect compiles to Lua.",
	"main": "./dist/esm/e.mjs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pigpigyyy/E.git"
	},
	"keywords": ["lua", "cpp", "moonscript", "e", "transpiler"],
	"exports": {
		".": {
			"import": "./dist/esm/e.mjs",
			"require": "./dist/cjs/e.cjs",
			"types": "./dist/esm/e.d.ts"
		},
		"./e.wasm": {
			"import": "./dist/esm/e.wasm",
			"require": "./dist/cjs/e.wasm"
		},
		"./e.wasm.map": {
			"import": "./dist/esm/e.wasm.map",
			"require": "./dist/cjs/e.wasm.map"
		}
	},
	"files": [
		"./dist/cjs/e.cjs",
		"./dist/esm/e.mjs",
		"./dist/esm/e.d.ts",
		"./dist/esm/e.wasm",
		"./dist/esm/e.wasm.map",
		"./dist/cjs/e.d.ts",
		"./dist/cjs/e.wasm",
		"./dist/cjs/e.wasm.map"

	],
	"browser": {
		"child_process": false,
		"crypto": false,
		"fs": false,
		"fs/promises": false,
		"path": false,
		"url": false,
		"vm": false,
		"ws": false
	},
	"author": "pigpigyyy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pigpigyyy/E/issues"
	},
	"homepage": "https://github.com/pigpigyyy/E#readme",
	"tsd": {
		"compilerOptions": {
			"lib": ["ES2017", "DOM"]
		}
	},
	"types": "./dist/esm/e.d.ts",
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {},
	"scripts": {}
}
